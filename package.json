{
  "name": "ateaparty",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "postinstall": "simple-git-hooks",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier -w --write .",
    "lint:eslint": "eslint . --ext js,jsx,ts,tsx --max-warnings=5",
    "lint:prettier": "prettier --check .",
    "test": "jest",
    "fix": "nr format && eslint --fix .",
    "find-deadcode": "knip",
    "precommit": "ni --frozen && nr lint:eslint && nr lint:prettier",
    "ci": "nr precommit && nr find-deadcode && nr test && nr build"
  },
  "simple-git-hooks": {
    "pre-commit": "nr precommit"
  },
  "dependencies": {
    "@rebass/preset": "4.0.5",
    "@tippyjs/react": "4.2.6",
    "axios": "1.7.2",
    "comma-separated-values": "3.6.4",
    "firebase": "10.12.3",
    "next": "14.2.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-headroom": "3.2.1",
    "react-markdown": "9.0.1",
    "react-twitter-embed": "4.0.4",
    "rebass": "4.0.7",
    "remixicon": "^4.3.0",
    "styled-components": "6.1.11",
    "tailwindcss": "^3.4.6"
  },
  "devDependencies": {
    "@types/jest": "29.5.12",
    "@types/react": "18.3.3",
    "@types/react-headroom": "3.2.3",
