    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-react": "7.34.4",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-unused-imports": "3.2.0",
    "jest": "29.7.0",
    "knip": "^5.26.0",
    "postcss": "^8.4.39",
    "prettier": "3.3.3",
    "simple-git-hooks": "2.11.1",
    "ts-jest": "29.2.2",
    "typescript": "5.5.3"
  }
